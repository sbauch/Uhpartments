<div class="topbar">
      <div class="fill">
        <div class="container">
          	<div class="brand"><%= link_to "Uhpartments for Brokers", '/brokers' %>
		</div>
          <ul class="nav">
		    <li><%= link_to "Sign In", new_user_session_path %></li>
          	<li><%= link_to "Sign Up", new_user_registration_path %></li> 
         </ul>
        </div>
      </div>
    </div>
<div class="container">
  <div class="content">
	<div class="page-header">
		<center><h1>Uhpartments for Brokers</h1></center>
	</div>
<div class="row">
<div class="span8">
	<h3><center>30 Day Free Trial<br>$49 per month thereafter</h3>
		<br>
<%= twitter_bootstrap_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>

<%= f.email_field :email, :placeholder => 'me@example.com' %>

<%= f.text_field :first_name %>


<%= f.text_field :last_name %>

<%= f.password_field :password %>

<%= f.password_field :password_confirmation, 'Confirm Password' %>

<%= f.text_field :company_name %>

<%= f.text_field :address %>

<%= f.text_field :address_alt %>

<%= f.text_field :city %>

<%= f.text_field :state, :class => 'small' %> <%= f.text_field :zip, :class => 'small' %>


<center><%= f.submit "Next" %></center>
<% end %>
</div>
<div class="span4">
	<br><br><br><br>
	<h3>Questions?</h3>
	<br>
	<h3>Email us:<br>clients@uhpartments.com</h3>